{
  "name": "api",
  "description": "Feathers.js api boilerplate",
  "version": "0.0.1",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "justlo0king",
    "email": "info@way.systems"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^14.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --fix",
    "dev": "nodemon src/",
    "start": "node src/",
    "mocha": "npx mocha -r esm test/ --recursive --exit",
    "test-app": "npx mocha -r esm test/app.test.js",
    "test-coinrate": "npx mocha -r esm test/modules/coinrate.test.js",
    "test-socketio": "npx mocha -r esm test/modules/socketio.test.js",
    "test-connections": "npx mocha -r esm test/modules/connections.test.js"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-memory": "^4.1.0",
    "helmet": "^4.2.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "axios": "^0.21.1",
    "eslint": "^7.16.0",
    "esm": "^3.2.25",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6"
  }
}
